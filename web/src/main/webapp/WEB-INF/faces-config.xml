<?xml version="1.0" encoding="UTF-8"?>
<faces-config
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
						http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<!-- Managed Beans -->

	<lifecycle>
		<phase-listener>org.gov.web.listener.Autorizador</phase-listener>

	</lifecycle>


	<managed-bean>
		<managed-bean-name>livroBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.LivroBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>apacBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.ApacBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>cnarhBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.CnarhBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.LoginBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>interferenciaBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.InterferenciaBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>asbcaracBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.AsbcaracBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>usuarioBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.UsuarioBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

		<managed-bean>
		<managed-bean-name>empreendimentoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.EmpreendimentoBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<!-- Configurações -->

	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>application-message</base-name>
			<var>msg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>application-error-message</base-name>
			<var>msg_error</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>application-tooltip-message</base-name>
			<var>msg_tip</var>
		</resource-bundle>
	</application>

</faces-config>