<?xml version="1.0" encoding="UTF-8"?>
<faces-config
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
						http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<!-- Managed Beans -->

	<lifecycle>
		<phase-listener>org.gov.web.listener.Autorizador</phase-listener>
	</lifecycle>


	<managed-bean>
		<managed-bean-name>fileUploadBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.FileUploadBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>apacBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.ApacBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>cnarhBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.CnarhBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.LoginBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>interferenciaBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.InterferenciaBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>asbcaracBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.AsbcaracBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>asbamaBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.AsbamaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>asbtstBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.AsbtstBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>captacaoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.CaptacaoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>consumohumanoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.ConsumoHumanoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>industriaBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.IndustriaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>irrigacaoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.IrrigacaoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>lancamentoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.LancamentoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>pecuariaBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.PecuariaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>testebombeamentoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.TesteBombeamentoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>vazaoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.VazaoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>usuarioBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.UsuarioBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>empreendimentoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.EmpreendimentoBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>fornecedorBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.FornecedorBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>outrainfoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.OutraInfoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>fichaCampoBean</managed-bean-name>
		<managed-bean-class>org.gov.web.controller.FichaCampoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>


	<validator>
		<validator-id>cnpjValidator</validator-id>
		<validator-class>org.gov.web.validator.CnpjValidador</validator-class>
	</validator>

	<validator>
		<validator-id>horaValidator</validator-id>
		<validator-class>org.gov.web.validator.HoraValidator</validator-class>
	</validator>

	<validator>
		<validator-id>mesComTrintaDiaValidator</validator-id>
		<validator-class>org.gov.web.validator.MesComTrintaDiaValidator</validator-class>
	</validator>

	<validator>
		<validator-id>mesFevereiroDiaValidator</validator-id>
		<validator-class>org.gov.web.validator.MesFevereiroDiaValidator</validator-class>
	</validator>

	<validator>
		<validator-id>cepValidator</validator-id>
		<validator-class>org.gov.web.validator.CepValidator</validator-class>
	</validator>

	<validator>
		<validator-id>mesComTrintaUMDiaValidator</validator-id>
		<validator-class>org.gov.web.validator.MesComTrintaUMDiaValidator</validator-class>
	</validator>

	<validator>
		<validator-id>quantidadeDBOValidator</validator-id>
		<validator-class>org.gov.web.validator.QuantidadeDBOValidator</validator-class>
	</validator>

	<validator>
		<validator-id>emailValidator</validator-id>
		<validator-class>org.gov.web.validator.EmailValidator</validator-class>
	</validator>

	<validator>
		<validator-id>cpfValidator</validator-id>
		<validator-class>org.gov.web.validator.CPFValidador</validator-class>
	</validator>

	<converter>
		<converter-id>cnpjConvert</converter-id>
		<converter-class>org.gov.web.converter.CnpjConverter</converter-class>
	</converter>

	<!-- Configurações -->

	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>application-message</base-name>
			<var>msg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>application-error-message</base-name>
			<var>msg_error</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>application-tooltip-message</base-name>
			<var>msg_tip</var>
		</resource-bundle>
	</application>

</faces-config>